{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "slim/slim": "^3.1",
        "monolog/monolog": "^1.17",
        "aura/accept": "^2.2",
        "shrikeh/teapot": "^2.2",
        "league/event": "^2.1",
        "slim/twig-view": "^2.2",
        "phpunit/phpunit": "^6.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "brunty/api-testcase": "^1.0",
        "ciaranmcnulty/behat-psr7extension": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
	"scripts": {
        "compile": [
            "cp var/storage/cages.stub.json var/storage/cages.json"
        ]
	}

}
